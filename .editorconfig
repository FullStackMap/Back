[*.cs]

# CA1305: Specify IFormatProvider
dotnet_diagnostic.CA1305.severity = none

# CS8604: Possible null reference argument.
dotnet_diagnostic.CS8604.severity = none

# ConstructorDocumentationHeader: The constructor must have a documentation header.
dotnet_diagnostic.ConstructorDocumentationHeader.severity = none

# ClassDocumentationHeader: The class must have a documentation header.
dotnet_diagnostic.ClassDocumentationHeader.severity = none

# InterfaceDocumentationHeader: The interface must have a documentation header.
dotnet_diagnostic.InterfaceDocumentationHeader.severity = none

# PropertyDocumentationHeader: The property must have a documentation header.
dotnet_diagnostic.PropertyDocumentationHeader.severity = warning